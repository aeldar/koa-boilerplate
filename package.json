{
  "name": "killme-koa",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register",
    "clean": "rimraf dist",
    "start": "nodemon src/server.js --exec babel-node",
    "start:debug": "nodemon src/server.js --exec babel-node-debug",
    "build:dist": "babel src -d dist --source-maps inline",
    "build": "npm run clean && npm run build:dist",
    "serve": "node dist/server.js",
    "serve:debug": "node-debug --hidden node_modules/ --hidden .test.js$ dist/server.js",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "keywords": [],
  "author": "Eldar Aliyev <eldar.aliyev8@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "koa": "^2.0.0",
    "koa-router": "^7.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-node-debug": "^2.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.16.3",
    "flow-bin": "^0.33.0",
    "mocha": "^3.1.0",
    "node-inspector": "^0.12.8",
    "nodemon": "^1.10.2",
    "rimraf": "^2.5.4"
  }
}
